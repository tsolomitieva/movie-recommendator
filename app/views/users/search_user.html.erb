<div class="container ">
  <%= search_form_for @q, url: search_user_users_path, method: :get do |f| %>
    <div class="input-group">
      <%= f.text_field :username_eq, class: "form-control ", placeholder: "Insert username" %>
      <%= f.submit class: "btn btn-outline-light " %>
    </div>
  <%end%>
  <div class="row mt-5">
    <ul>
      <% @users.each do |user| %>
        <li> <%= user.username%></li>
      <%end%>
    </div>
  </ul>
</div>
